{
    "name": "spatie/laravel-event-sorcerer",
    "description": "Simple event sourcing in Laravel",
    "keywords": [
        "spatie",
        "laravel-event-sourcerer",
        "event",
        "sourcing"
    ],
    "homepage": "https://github.com/spatie/laravel-event-sorcerer",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "5.6.*",
        "illuminate/events": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EventSourcerer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\EventSourcerer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\EventSourcerer\\EventSourcererServiceProvider"
            ],
            "aliases": {
                "EventSourcerer": "Spatie\\EventSourcerer\\Facades\\EventSourcerer"
            }
        }
    }
}
