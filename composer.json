{
    "name": "spatie/laravel-event-saucer",
    "description": "Simple event saurcing in Laravel",
    "keywords": [
        "spatie",
        ":package_name"
    ],
    "homepage": "https://github.com/spatie/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EventSaucer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\EventSaucer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\EventSaucer\EventSaucerServiceProvider"
            ]
        }
    }
}
